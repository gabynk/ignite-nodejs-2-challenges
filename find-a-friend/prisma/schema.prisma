// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Corporation {
  id         String   @id @default(uuid())
  name       String
  email      String   @unique
  password   String
  cep        String
  address    String
  phone      String
  created_at DateTime @default(now())

  @@map("corporations")
}
